import { datadogLogs } from "@datadog/browser-logs";
import DataDogLoggerService from "./logger-service";

global.logger = new DataDogLoggerService(datadogLogs, "<%= data[:constants][:datadog][:apiKey] %>");

angular.module("BitriseWorkflowEditor").factory("logger", function() {
	return global.logger;
});

export default global.logger;
